                  <li><a class="dropdown-item" href="#">Progress</a></li>
                  <li><a class="dropdown-item" href="#">Settings</a></li>
                  <li><hr class="dropdown-divider" /></li>
                  <li><a class="dropdown-item" href="#">Blog</a></li>
                  <li><a class="dropdown-item" href="#">Upgrade</a></li>
                  <li>
                    <a class="dropdown-item" href="#">Help and feedback</a>
                  </li>
                  <li><hr class="dropdown-divider" /></li>
                  <li><a class="dropdown-item" href="#">Log out</a></li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </nav>
      <!-- Modal -->
    </nav>

    <div class="container">
      <h1 class="bach"></h1>
      <div class="row">
        <div class="col-3 tolbar">
          <i class="fa-solid fa-copy"></i>
          <span class="title-toobar">Flashcards</span>
        </div>
        <div class="col-3 tolbar">
          <i class="fa-solid fa-spinner"></i>
          <span class="title-toobar">Learn</span>
        </div>
        <div class="col-3 tolbar">
          <i class="fa-solid fa-file"></i>
          <span class="title-toobar">Test</span>
        </div>
        <div class="col-3 tolbar">
          <i class="fa-solid fa-laptop-file"></i>
          <span class="title-toobar">Match</span>
        </div>
      </div>
    </div>
<div class="daada">
    <div id="carouselExampleControlsNoTouching" class="carousel slide carousel12" data-bs-touch="false">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <div class="container">
            <div class="card" id="flip-btn0">
              <div id="back0" class="cardBack"><p class="back"><%= data[0].term %> </p></div>
              <div id="front0" class="cardFront"><p class="front"><%= data[0].definition %></p></div>
            </div>
          </div>
        </div>
        <% for( let i = 1; i < data.length; i++ ) { %>
          <div class="carousel-item">
            <div class="container">
              <div class="card" id="flip-btn<%=i%>">
                <div id="back<%=i%>" class="cardBack"><p class="back"><%= data[i].term %> </p></div>
                <div id="front<%=i%>" class="cardFront"><p class="front"><%= data[i].definition %></p></div>
              </div>
            </div>
          </div>
        <% } %>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
    
  </div>
